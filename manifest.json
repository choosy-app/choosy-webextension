{
  "name": "Choosy",
  "manifest_version": 2,
  "version": "1.2.2",
  "description": "Send pages and links to another browser using Choosy (Mac OS X only).",
  "icons": { "16": "images/icon16.png",
             "48": "images/icon48.png",
            "128": "images/icon128.png" },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "tooltip": "Open with Choosy"
  },
  "permissions": ["contextMenus"]
}
